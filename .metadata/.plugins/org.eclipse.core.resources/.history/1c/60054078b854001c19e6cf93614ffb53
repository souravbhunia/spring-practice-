package com.spring;

public class student {

	
	private int studentId;
	private String studentName;
	private String studentAddress;
	
	
	public int getStueentId() {
		return stueentId;
	}
	public void setStueentId(int stueentId) {
		this.stueentId = stueentId;
	}
	public String getStudentName() {
		return studentName;
	}
	public void setStudentName(String studentName) {
		this.studentName = studentName;
	}
	public String getStudentAddress() {
		return studentAddress;
	}
	public void setStudentAddress(String studentAddress) {
		this.studentAddress = studentAddress;
	}
	public student(int stueentId, String studentName, String studentAddress) {
		super();
		this.stueentId = stueentId;
		this.studentName = studentName;
		this.studentAddress = studentAddress;
	}
	public student() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "student [stueentId=" + stueentId + ", studentName=" + studentName + ", studentAddress=" + studentAddress
				+ "]";
	}
	 
	
	
	
	
	
}
